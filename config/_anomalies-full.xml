                        <fingerprint>PreparedStatement</fingerprint>
                        <fingerprint>(java\.sql)([\.;])([a-zA-Z_0-9]*\.)*([a-zA-Z_0-9]*|\*)*(;){0,1}</fingerprint>
                        <fingerprint>(javax\.sql)([\.;])([a-zA-Z_0-9]*\.)*([a-zA-Z_0-9]*|\*)*(;){0,1}</fingerprint>
