<config>
    <name>abnamro</name>
    <version>1</version>
    <data>
        <languages>
            <language>
                <name>java</name>
                <version></version>
                <technology>
                    <name>Excessive Complexity</name>
                    <category>
                        <name>Autogenerated Javadoc</name>
                        <fingerprint>(?i)Auto-generated\s+Javadoc</fingerprint>
                    </category>
                </technology>

                <technology>
                    <name>Abnormal Hierarchy Relations</name>
                    <category>
                        <name>Instanceof Abuser</name>
                        <fingerprint>instanceof</fingerprint>
                    </category>
                    <category>
                        <name>Stub Implementer</name>
                        <fingerprint>(?i)Auto-generated method stub</fingerprint>
                        <fingerprint>\{\s*\/\/Overridden Notification Method\s*\}</fingerprint>
                        <fingerprint>\{\s*return null;\s*\}</fingerprint>
                    </category>
                </technology>

                <technology>
                    <name>Traceability Issues</name>
                    <category>
                        <name>Console Writing</name>
                        <fingerprint>System\.out\.print</fingerprint>
                        <fingerprint>System\.err\.print</fingerprint>
                    </category>
                    <category>
                        <name>Stacktrace Dump</name>
                        <fingerprint>printStackTrace\(</fingerprint>
                    </category>
                    <category>
                        <name>Catch Top-Level Exception</name>
                        <fingerprint>catch\s*\(\s*Exception</fingerprint>
                        <fingerprint>catch\s*\(\s*Error</fingerprint>
                        <fingerprint>catch\s*\(\s*Throwable</fingerprint>
                    </category>
                </technology>

                <technology>
                    <name>Unit Testing</name>
                    <category>
                        <name>Unit Test Files</name>
                        <fingerprint>(org\.junit)([\.;])([a-zA-Z_0-9]*\.)*([a-zA-Z_0-9]*|\*)*(;){0,1}</fingerprint>
                        <fingerprint>(junit\.framework)([\.;])([a-zA-Z_0-9]*\.)*([a-zA-Z_0-9]*|\*)*(;){0,1}
                        </fingerprint>
                        <fingerprint>(org\.hamcrest)([\.;])([a-zA-Z_0-9]*\.)*([a-zA-Z_0-9]*|\*)*(;){0,1}</fingerprint>
                    </category>
                </technology>

